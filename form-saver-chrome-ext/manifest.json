{
  "name": "Form Saver",
  "version": "1.4",
  "manifest_version": 3,
  "description": "Save and fill form",
  "action": {
    "default_popup": "popup.html",
    "default_icon": "icon-32.png",
    "default_title": "Form Saver"
  },
  "icons": {
    "16": "assets/icon-16.png",
    "32": "assets/icon-32.png",
    "48": "assets/icon-48.png"
  },
  "permissions": [
    "activeTab",
    "scripting"
  ],
  "background": {
    "service_worker": "background.js"
  }
}